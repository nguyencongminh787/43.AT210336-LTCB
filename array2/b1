#include<stdio.h>
int main(){
    int row = 3;
    int col = 3;
    int arr[row][col];
    for(int i =0;i<row;i++){
        for(int j = 0; j<col;j++){
            printf("\n");
            printf("nhập đê [%d][%d]",i,j);
            scanf("%d", &arr[i][j]);
        }
    }
    printf("\n");
    for(int i = 0; i<row;i++){
        for(int j = 0; j<col;j++){
            printf("%d ",arr[i][j]);
        }
        printf("\n");
    }
    return 0;
}
