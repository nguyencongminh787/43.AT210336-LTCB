#include <stdio.h>
int main(){
    float t ;
    scanf ("%f", &t);
    if(t >= 0 && t <= 10){
        if(t<5){
        printf("YẾU");
    }
    if(t >= 5 && t <7){
        printf("TB");
    }
    if(t >= 7 && t < 8){
        printf("KHA");
    }if(t >= 8 && t < 9){
        printf("GIOI");
    }if(t >= 9){
        printf("XUẤT SẮC");
    }}else{
        printf("NHẬP SAI ĐIỂM");
    }
}
