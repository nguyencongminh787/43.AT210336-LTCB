#include <stdio.h>
#include <math.h>
int main(){
    float a , b,c,p,s,t;
    scanf("%f %f %f", &a,&b,&c);
    if(a+b>c || b+c>a || c+a>b){
        if(a== b && b==c){
            printf("TAM GIAC ĐỀU\n");
        }else if(b != c && a == b || c!= a && a == b || a!=b && c == b){
            printf("Tam giác cân\n");
        if( a*a + b*b  == c*c || a*a + c*c == b*b || b*b + c*c == a*a ){
            printf("Vuông cân\n");
            
        }}else if( a*a + b*b  == c*c || a*a + c*c == b*b || b*b + c*c == a*a  ){
            printf("Vuông\n");
        }else{
            printf("Thường\n");
        }
        t= a+b+c;
        p = t/2;
        s = sqrt(p*(p-a)*(p-b)*(p-c));
        printf("Chu vi tam giác là %.2f\n", t);
        printf("Diện tích tam giác là %.2f\n",s);
        
    }else{
            printf("Không phải tam giác");
        }
    }
